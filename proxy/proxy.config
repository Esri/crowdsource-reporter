<?xml version="1.0" encoding="utf-8" ?>
<ProxyConfig allowedReferers="*"
             logFile="../log/proxylog.txt" logLevel="Verbose"
             mustMatch="true">
    <serverUrls>
        <serverUrl url="http://services.arcgisonline.com"
                   matchAll="true"/>
        <serverUrl url="http://services6.arcgis.com/Pu6Fai10JE2L2xUd"
                   matchAll="true"/>
        <serverUrl url="https://services6.arcgis.com/Pu6Fai10JE2L2xUd"
                   matchAll="true"/>
    </serverUrls>
    <shieldedFields>
        <shieldedField name="NAME" replacement="(private name)"/>
        <shieldedField name="USERID" replacement="(private userid)"/>
        <shieldedField name="PHONE" replacement="(private phone)"/>
        <shieldedField name="EMAIL" replacement="(private email)"/>
    </shieldedFields>
</ProxyConfig>

